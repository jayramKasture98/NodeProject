<h3><a class ="btn btn-secondary" href="/employee"><i class="fa fa-plus"></i>Create New</a>Employee Details</h3>
<table class ="table table-striped">
    <thead>
        <tr>
            <th> Full Name</th>
            <th>Email</th>
            <th>Mobile</th>
            <th>City</th>
        </tr>
    </thead>
    <tbody>
        {{#each list}}
        <tr>
            <td>{{this.fullname}}</td>
            <td>{{this.email}}</td>
            <td>{{this.mobile}}</td>
            <td>{{this.city}}</td>  
        </tr>
        {{/each}}
    </tbody>
</table>